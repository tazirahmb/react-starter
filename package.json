{
  "name": "react-codebase-starter",
  "version": "1.0.5",
  "main": "index.js",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "open:src": "babel-node tools/srcServer.js",
    "serve": "concurrently -k -r \"npm:open:src\" \"npm:lint:watch\"",
    "version": "auto-changelog -p && git add changelog.md",
    "lint": "esw src/ --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:commit": "npm run lint -- --quiet",
    "lint-staged": "lint-staged",
    "test": "jest --updateSnapshot --runInBand",
    "test:commit": "jest --updateSnapshot --runInBand --silent --passWithNoTests ./src",
    "test:cover": "npm run test -- --coverage ",
    "build": "babel-node tools/build.js",
    "build:development": "npm run build -- --development",
    "build:staging": "npm run build -- --staging",
    "build:production": "npm run build -- --production",
    "start": "babel-node tools/distServer.js",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/node": "^7.22.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "auto-changelog": "^2.4.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "chalk": "^5.3.0",
    "connect-gzip-static": "^3.0.0",
    "connect-history-api-fallback": "^2.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-watch": "^8.0.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "raf": "^3.4.1",
    "react-hot-loader": "^4.13.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "concurrently": "^8.2.1",
    "connected-react-router": "^6.9.3",
    "formik": "^2.4.3",
    "history": "^5.3.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "redux": "^4.2.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.4.2",
    "typeface-ubuntu": "^1.1.13",
    "voca": "^1.4.1",
    "yup": "^1.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "npm run lint:commit"
    ],
    "*": [
      "npm run test:commit"
    ]
  }
}
